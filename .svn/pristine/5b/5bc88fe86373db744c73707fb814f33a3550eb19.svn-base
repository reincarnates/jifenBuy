.cartMain{
  position: relative;
  width: 1200px;
  min-width: 1200px;
  max-width: 1200px;
  margin: 0 auto;
  padding: 0px 0px 100px;
  min-height: 210px;
}
/*购物车头部*/
.cartMain_hd{
  width: 100%;
  height: 50px;
  line-height: 50px;
  color: #3c3c3c;
}
.cartMain_hd .cartTop{
  height: 50px;
}
.cartMain_hd .cartTop .list_chk{
  width: 80px;
  text-indent: 30px;
}
.cartMain_hd .cartTop .list_con{
  width: 312px;
}
.cartMain_hd .cartTop .list_chk label{
  position: absolute;
  left: 10px;
  top:19px;
  margin: 0;
}
.cartMain_hd .cartTop .list_info{
  padding: 0;
  text-indent: 15px;
}
.cartMain_hd .cartTop .list_con{
  text-indent: 140px;
}


.cartBox{
  width: 100%;
  /* margin-bottom: 15px; */
}
.cartBox .shop_info{
  position: relative;
  width: 100%;
  height: 61px;
  background-color: #fff;
  line-height: 61px;
  vertical-align: baseline;
}
.cartBox .shop_info .all_check{
  position: relative;
  float: left;
  width: 30px;
  height: 38px;
}

.cartBox .shop_info .all_check input[type="checkbox"]{
  position: absolute;
  z-index: 0;
  left: 10px;
  top: 21px;
}
.cartBox .shop_info .all_check .shop{
  position: absolute;
  top:21px;
}
.cartBox .shop_info .shop_name{
  float: left;
  margin-left: 6px;
  color: #333;
  font-size: 14px;
}



/*商品列表*/
.cartBox .order_content{
  border: 1px solid #ebebeb;
}
.cartBox .order_content a{
  display: block;
}
.order_lists{
  width: 100%;
  height: 125px;
  border-bottom: 1px solid #ebebeb;
}
.order_lists:last-child{
  border-bottom: none;
}
.order_lists li{
  float: left;
  height: 100%;
}

.order_lists .list_chk{
  position: relative;
  width: 50px;
}
.order_lists .list_chk input[type="checkbox"]{
  position: absolute;
  z-index: 0;
  left: 10px;
  top: 55px;
}
.order_lists .list_chk label{
  margin: 55px 0 0 10px;
}

.order_lists .list_con{
  width: 342px;
}
.order_lists .list_con .list_img{
  width: 80px;
  height: 80px;
  margin-top: 20px;
  float: left;
}
.order_lists .list_con .list_img img{
  width: 100%;
  vertical-align: top;
}
.order_lists .list_con .list_text{
  margin: 30px 0 0 0;
  text-indent: 5px;
  color: #333;
  font-size: 12px;
  line-height: 21px;
  width: 200px;
  float: left;
}
.order_lists .list_con .list_text a{
  color: #333;
}
.order_lists .list_con .list_text a:hover{
  color: #ff873e;
  text-decoration: underline;
}

.order_lists .list_info{
  width: 252px;
  box-sizing: border-box;
  padding: 20px 0;
}
.order_lists .list_info p{
  color: #9c9c9c;
  line-height: 18px;
  margin-left: 15px;
}
.order_lists .list_price{
  width: 223px;
  text-align: center;
}
.order_lists .list_price .oldprice{
  color: #333;
  font-size: 14px;
  text-decoration: line-through;
  margin-top: 35px;
  line-height: 24px;
}
.order_lists .list_price .newPrice{
  font-size: 18px;
  color: #333;
  line-height: 24px;
}
.order_lists .list_amount{
  width: 204px;
}
.order_lists .list_amount .amount_box{
  margin-top: 55px;
  width: 109px;
  height: 22px;
  position: relative;
  margin-left: 45px;
}
.order_lists .list_amount .amount_box input{
  width: 63px;
  height: 22px;
  line-height: 22px;
  border-style: solid;
  border-color: #d1d1d1;
  border-width: 1px 0 1px 0;
  color: #333;
  text-align: center;
  padding: 4px 0;
  background-color: #fff;
  z-index: 2;
  position: absolute;
  left: 23px;
  float: left;
}
.order_lists .list_amount .amount_box a{
  float: left;
  height: 22px;
  width: 23px;
  border: 1px solid #d1d1d1;
  background: url(../../images/static/setting.png) no-repeat;
  position: absolute;
  top:0;
}
.order_lists .list_amount .amount_box a:hover{
  border-color: #ff873e;
  text-decoration: none;
  color: #ff873e;
  z-index: 3;
}

.order_lists .list_amount .amount_box .reduce{
  left: 0;
  background-position: 5px 5px;
}

.order_lists .list_amount .amount_box .reSty{
  color: #cbcbcb;
}
.order_lists .list_amount .amount_box .reSty:hover{
  border-right: none;
  border-color: #e5e5e5;
  text-decoration: none;
  color: #cbcbcb;
}

.order_lists .list_amount .amount_box .plus{
  right: 0;
  background-position: -16px 5px;
}


.order_lists .list_sum{
  width: 167px;
  text-align: center;
}
.order_lists .list_sum .sum_price{
  line-height: 125px;
  color: #333;
  font-size: 18px;
}
.order_lists .list_op{
  /* width: 164px; */
  line-height: 22px;    
  color: #333;
  font-size: 12px;
  padding-left: 115px;
  box-sizing: border-box;
}
.order_lists .list_op .del{
  margin-top: 43px;
  line-height: 18px;
}

/*底部总计算价*/
.bar-wrapper{
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0px;
  z-index: 9;
  background-color: #fff;
}
.bar-content{
  width: 1200px;
  height: 65px;
  margin: 0 auto;
}
.bar-wrapper .bar-right{
  float: right;
  font-size: 12px;
  color: #999;
}
.bar-wrapper .bar-right strong{
  color: #f40;
}

.bar-wrapper .bar-right .piece{
  float: left;
  min-width: 110px;
  /* margin-right: 20px; */
  height: 65px;
  line-height: 65px;
}
.bar-wrapper .bar-right .piece .piece_num{
  display: inline-block;
  color: #ff4600;
  /* padding: 0 10px; */
  /* font-weight: 700; */
  /* font-size: 18px; */
  /* font-family: tohoma,arial; */
}
.bar-wrapper .bar-right .totalMoney{
  float: left;
  min-width: 100px;
  height: 65px;
  line-height: 65px;
}
.bar-wrapper .bar-right .totalMoney .total_text{
  /* float: right; */
  font-weight: 400;
  font-size: 16px;
  color: #ff5826;
  font-family: Arial;
  vertical-align: middle;
  margin-right: 10px;
  /* margin-left: 15px; */
}
.bar-wrapper .bar-right .calBtn{
  float: left;
}
.bar-wrapper .bar-right .calBtn button{
  display: block;
  width: 103px;
  height: 65px;
  color: #fff;
  background: #B0B0B0;
  cursor: not-allowed;
  font-size: 16px;
  letter-spacing: 5px;
  text-decoration: none;
  line-height: 65px;
  text-align: center;
  border: 1px solid #d1d1d1;
  outline: none;
}
.bar-wrapper .bar-right .calBtn button.btn_sty{
  background: #ff5826;
  cursor: pointer;
}

.not-cart-wrapper{
  width: 100%;
  height: 575px;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.not-cart-btn{
  width: 104px;
  height: 36px;
  border: 1px solid #ddd;
  text-align: center;
  line-height: 36px;
  font-size: 14px;
  color: #666;
  margin-top: 33px;
}

.not-cart-btn:hover{
  background-color: #ffb22c;
  border-color: #ffb22c;
  color: #fff;
  cursor: pointer;
}