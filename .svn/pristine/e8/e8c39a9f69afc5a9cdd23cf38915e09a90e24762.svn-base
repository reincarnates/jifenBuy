@charset "utf-8";

* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

html,
body{
    width: 100%;
    height: 100%;
}

body{
    font-size: 13px;
    font-family: "Microsoft YaHei";
    color: #666666;
}

a:visited {
    color: #555555;
    text-decoration: none;
}

a:link {
    color: #555555;
    text-decoration: none;
}

ol,
ul {
    list-style: none;
}

h1, h2, h3, h4, h5, h6 {
    font-size: 100%;
}

img {
    border: 0;
    /* vertical-align: middle; */
}
