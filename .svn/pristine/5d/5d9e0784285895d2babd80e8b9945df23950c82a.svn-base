@charset "utf-8";

.shipped-container{
  width: 1200px;
  margin: 0 auto;
}

.shipped-head{
  width: 100%;
  line-height: 34px;
}

.shipped-head a{
  font-size: 14px;
  color: #999;
}

.shipped-head-color{
  color: #333 !important;
}

.shipped-content-wrapper{
  width: 100%;
  padding: 0 55px;
  box-sizing: border-box;
}

.shipped-step-wrapper{
  width: 100%;
  position: relative;
}

.shipped-step-bg{
  width: 180px;
  height: 1px;
  /* border: 1px dashed #5dc442; */
  border: 1px dashed #dbdbdb;
  position: absolute;
  top: 55px;
  left: 65px;
}

.shipped-step-progress{
  border-color: #5dc442;
}

.shipped-step-position{
  left: 284px;
}

.shipped-step-position2{
  left: 502px;
}

.shipped-step-position3{
  left: 720px;
}

.shipped-step{
  height: 88px;
  position: relative;
  margin-top: 18px;
}

.shipped-step-item{
  float: left;
  width: 20%;
  height: 88px;
  padding-left: 10px;
  z-index: 9;
}

.shipped-step-item-active{
  border-color: #65c74b !important;
}

.shipped-step-item-title{
  line-height: 30px;
  /* text-align: center; */
  color: #333;
  font-size: 14px;
}

.shipped-step-item-title-next{
  color: #999;
}

.shipped-step-item-line{
  line-height: 47px !important;
  margin-top: 5px;
}

.shipped-step-item-receipt{
  line-height: 38px !important;
}

.shipped-step-item-num{
  line-height: 58px;
}

.shipped-step-item-num span{
  width: 42px;
  height: 42px;
  line-height: 43px;
  text-align: center;
  border: 3px solid #d8d8d8;
  display: inline-block;
  border-radius: 50%;
  margin-left: 15px;
}

.shipped-current-status-wrapper{
  width: 100%;
  height: 91px;
  border: 1px solid #e9e9e9;
  margin-top: 31px;
  padding: 0 18px;
  box-sizing: border-box;
}

.shipped-current-status-left{
  width: 476px;
  float: left;
}

.shipped-current-status{
  line-height: 26px;
  position: relative;
  margin-top: 17px;
}

.shipped-current-status-icon{
  width: 26px;
  height: 26px;
  display: inline-block;
  background: url(../../images/static/warning.png) no-repeat;
  position: absolute;
}

.shipped-current-status-word{
  font-size: 16px;
  color: #333;
  margin-left: 32px;
}

.shipped-current-status-word2{
  font-size: 14px;
  color: #999;
  margin-left: 32px;
  line-height: 36px;
}

.shipped-current-status-word2 span{
  color: #ff5826;
  margin: 0 10px;
}

.shipped-current-status-right{
  width: 83px;
  height: 33px;
  background-color: #ffb030;
  text-align: center;
  line-height: 33px;
  color: #fff;
  font-size: 14px;
  margin-top: 29px;
  cursor: pointer;
  border-radius: 1px;
  float: right;
}

.shipped-current-status-right-payment{
  width: 83px;
  height: 33px;
  background-color: #ff5826;
  text-align: center;
  line-height: 33px;
  color: #fff;
  font-size: 14px;
  margin-top: 29px;
  cursor: pointer;
  border-radius: 1px;
  margin-right: 18px;
  float: right;
}

.shipped-current-status-right-wrod{
  float: right;
  color: #666666;
  font-size: 14px;
  line-height: 91px;
  cursor: pointer;
}

.shipped-current-status-right-wrod:hover{
  color: #e98930;
  text-decoration: underline;
}

.shipped-current-status-right:hover{
  opacity: 0.8;
}

.shipped-current-status-right-payment:hover{
  opacity: 0.8;
}

.shipped-order-wrapper{
  width: 100%;
  height: 210px;
  border: 1px solid #e2e2e2;
  margin-top: 39px;
}

.shipped-order-head{
  width: 100%;
  height: 40px;
  border-bottom: 1px solid #e2e2e2;
  background-color: #fff6f3;
  line-height: 40px;
  color: #999;
  font-size: 14px;
  padding-left: 20px;
}

.order-time{
  margin-left: 44px;
}

.shipped-order-content{
  width: 100%;
  height: 159px;
}

.shipped-order-content-left{
  width: 839px;
  padding: 16px 14px;
  float: left;
}

.shipped-order-name-wrapper{
  line-height: 36px;
  font-size: 14px;
}

.shipped-order-name{
  color: #999;
}

.shipped-order-name-content{
  color: #333;
}

.shipped-order-content-right{
  width: 204px;
  padding-top: 27px;
  box-sizing: border-box;
  float: left;
}

.shipped-order-settlement{
  line-height: 24px;
}

.shipped-order-settlement-name{
  width: 70px;
  color: #999;
  font-size: 14px;
  text-align: right;
  display: inline-block;
}

.shipped-order-settlement-content{
  width: 128px;
  color: #333;
  text-align: right;
  display: inline-block;
}

.shipped-order-settlement-color{
  color: #ff5826;
  font-size: 18px;
  font-weight: bold;
}

.shipped-order-goods-wrapper{
  width: 100%;
  border: 1px solid #e2e2e2;
}

.shipped-order-goods-head{
  width: 100%;
  height: 40px;
  border-bottom: 1px solid #e2e2e2;
  background-color: #fff6f3;
  line-height: 40px;
  color: #999;
  font-size: 14px;
}

.shipped-order-goods-head span{
  display: inline-block;
  text-align: right;
}

.shipped-order-goods-head-info{
  width: 154px;
}

.shipped-order-goods-head-weight{
  width: 456px;
}

.shipped-order-goods-head-price{
  width: 206px;
}

.shipped-order-goods-head-number{
  width: 241px;
}

.shipped-order-goods-address{
  width: 100%;
  line-height: 39px;
  padding-left: 19px;
  font-size: 14px;
  color: #666;
  box-sizing: border-box;
  position: relative;
}

.shipped-order-goods-address-img{
  position: absolute;
  top: 10px;
  left: 20px;
}

.shipped-order-goods-address span{
  margin-left: 19px;
}

.shipped-order-goods-item-wrapper{
  width: 100%;
  height: 125px;
  border-bottom: 1px dashed #cdcdcd;
  padding: 24px 33px;
}

.shipped-order-goods-item-wrapper:last-child{
  border-bottom: none;
}

.shipped-order-goods-item-img{
  width: 81px;
  height: 81px;
  display: flex;
  align-items: center;
  justify-content: center;
  float: left;
}

.shipped-order-goods-item-introduce{
  width: 395px;
  line-height: 26px;
  margin-top: 15px;
  margin-left: 16px;
  float: left;
}

.shipped-order-goods-item-introduce a{
  color: #333333;
  font-size: 14px;
}

.shipped-order-goods-item-introduce a:hover{
  color: #e98930;
  text-decoration: underline;
}

.shipped-order-goods-item-weight{
  width: 113px;
  text-align: right;
  line-height: 81px;
  font-size: 14px;
  color: #333;
  float: left;
}

.shipped-order-goods-item-price{
  width: 205px;
  text-align: right;
  line-height: 81px;
  font-size: 14px;
  color: #333;
  float: left;
}

.shipped-order-goods-item-number{
  width: 210px;
  text-align: right;
  line-height: 81px;
  font-size: 14px;
  color: #333;
  float: left;
}

.shipped-current-status-wrapper2{
  width: 100%;
  height: 61px;
  border: 1px solid #e9e9e9;
  background-color: #fff6f3;
  margin-top: 31px;
  padding: 0 18px;
  box-sizing: border-box;
}

.shipped-current-status-right2{
  width: 83px;
  height: 33px;
  background-color: #fff;
  text-align: center;
  line-height: 33px;
  color: #666666;
  font-size: 14px;
  cursor: pointer;
  border-radius: 1px;
  border: 1px solid #ddd;
  margin-top: 13px;
  float: right;
}

.shipped-logistics-wrapper{
  width: 100%;
  border-style: solid;
  border-color: #e9e9e9;
  border-width: 0 1px 1px 1px;
  padding: 0 48px;
  box-sizing: border-box;
}

.shipped-logistics-word{
  width: 100%;
  line-height: 48px;
  color: #333;
  font-size: 14px;
}

.shipped-logistics-word-number{
  margin-left: 43px;
}

.track-rcol {
  width: 900px;
  /* height: 200px; */
}

.track-list {
  /* margin: 20px; */
  padding-left: 5px;
  position: relative;
  /* overflow-y: auto; */
}

.track-list li.first {
  color: #e98930;
  padding-top: 0;
  border-left-color: #fff;
}

.track-list li {
  position: relative;
  padding: 9px 0 0 25px;
  line-height: 18px;
  border-left: 1px solid #d9d9d9;
  color: #333;
  font-size: 12px;
}

.track-list li.first .node-icon {
  background-position: 0 -6px;
}

.track-list li .node-icon {
  position: absolute;
  left: -6px;
  top: 50%;
  width: 17px;
  height: 17px;
  background: url(../../images/static/order-icons.png) -3px 11px no-repeat;
  z-index: 1;
}

.track-list li.first .time {
  margin-right: 20px;
}

.track-list li .time {
  margin-right: 20px;
  position: relative;
  top: 4px;
  display: inline-block;
  vertical-align: middle;
}

.track-list li.first .txt {
  max-width: 600px;
}

.track-list li .txt {
  max-width: 600px;
  position: relative;
  top: 4px;
  display: inline-block;
  vertical-align: middle;
}

.retract{
  width: 100%;
  line-height: 54px;
  color: #333;
  font-size: 14px;
  cursor: pointer;
  background: url(../../images/static/retract.jpg) no-repeat;
  background-position: 35px 25px;
}

.shipped-current-status-receipt{
  width: 83px;
  height: 33px;
  background-color: #0090ff;
  text-align: center;
  line-height: 33px;
  color: #fff;
  font-size: 14px;
  cursor: pointer;
  border-radius: 1px;
  margin-top: 13px;
  float: right;
}