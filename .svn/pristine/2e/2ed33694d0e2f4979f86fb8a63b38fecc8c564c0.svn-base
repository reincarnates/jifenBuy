@charset "utf-8";

.coupon-wrapper {
  width: 980px;
  height: 972px;
  background-color: #fff;
  margin-left: 20px;
  float: left;
}

.coupon-head {
  width: 100%;
  line-height: 30px;
  border-bottom: 1px dashed #e5e5e5;
  padding-left: 20px;
  box-sizing: border-box;
  font-size: 12px;
  color: #666;
}

.coupon-content{
  width: 100%;
  height: 600px;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.coupon-word{
  width: 100%;
  line-height: 57px;
  color: #666;
  font-size: 16px;
  text-align: center;
}