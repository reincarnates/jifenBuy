}

.modal-frame{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .5);
  z-index: 999;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  /* display: none; */
}

.modal-frame2{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .5);
  z-index: 999;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: none;
}

.modal{
  position: fixed;
  left: 50%;
  /* top: -270px; */
  top: 50%;
  width: 764px;
  height: 460px;
  margin-left: -390px;
  margin-top: -200px;
  z-index: 1000;
  background-color: #fff;
  border: 2px solid #ddd;
}

.modal-head{
  width: 100%;
  height: 38px;
  background-color: #f3f3f3;
  padding: 0 21px;
  box-sizing: border-box;
  border-bottom: 1px solid #ddd;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 16px;
  color: #333;
}

.modal-content{
  width: 100%;
  padding: 17px 25px;
  box-sizing: border-box;
}

.modal-content-item{
  width: 100%;
}

.modal-content-item-word{
  width: 100%;
  line-height: 29px;
  color: #999;
  font-size: 13px;
}

.must{
  color: #ff0000;
}

.modal-content-item-input input{
  width: 234px;
  height: 32px;
  border: 1px solid #ddd;
  outline: none;
  padding-left: 5px;
}

.modal-content-item-select{
  width: 172px;
  height: 32px;
  border: 1px solid #ddd;
  padding-left: 5px;
}

.address_input{
  width: 498px !important;
}

.phone_number{
  width: 224px !important;
}

.fixed_phone{
  margin-left: 205px;
}

.huo{
  margin: 0 15px;
}

.modal-content-btn{
  width: 118px;
  height: 32px;
  border: 1px solid #ddd;
  background-color: #f4f3f3;
  color: #333;
  font-size: 14px;
  text-align: center;
  line-height: 30px;
  cursor: pointer;
  margin-top: 28px;
}

.modal-content-item-input{
  position: relative;
}

.city-picker-input {
  opacity: 0 !important;
  top: -9999px;
  left: -9999px;
  position: absolute;
}

.city-picker-span {
  position: relative;
  width: 215px !important;
  height: 33px !important;
  line-height: 33px !important;
  display: block;
  outline: 0;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  border: 1px solid #ccc;
  background-color: #fff;
  color: #ccc;
  cursor: pointer;
  background: url(../../images/static/order_select_address.png) no-repeat;
  background-position: 195px 13px;
}

.city-picker-span>.placeholder {
  color: #aaa;
  margin-left: 10px;
}

.city-picker-span>.arrow {
  position: absolute;
  top: 50%;
  right: 8px;
  width: 10px;
  margin-top: -3px;
  height: 5px;
  background: url(../../images/static/drop-arrow.png) -10px -25px no-repeat;
}

.city-picker-span.open>.arrow {
  background-position: -10px -10px;
}

.city-picker-span>.title>span {
  color: #333;
  padding: 5px;
  border-radius: 3px;
}

.city-picker-span>.title>span:hover {
  background-color: #f1f8ff;
}

.city-picker-dropdown {
  position: absolute;
  width: 315px;
  left: -9999px;
  top: -9999px;
  outline: 0;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  z-index: 999999;
  display: none;
  min-width: 330px;
  margin-bottom: 20px;
}

.city-select-wrap {
  box-shadow: 0 1px 5px rgba(0, 0, 0, 0.5);
}

.city-select-tab {
  border-bottom: 1px solid #ccc;
  background: #f0f0f0;
  font-size: 13px;
}

.city-select-tab>a {
  display: inline-block;
  padding: 8px 22px;
  border-left: 1px solid #ccc;
  border-bottom: 1px solid transparent;
  color: #4D4D4D;
  text-align: center;
  outline: 0;
  text-decoration: none;
  cursor: pointer;
  font-size: 14px;
  margin-bottom: -1px;
}

.city-select-tab>a.active {
  background: #fff;
  border-bottom: 1px solid #fff;
  color: #e98930;
}

.city-select-tab>a:first-child {
  border-left: none;
}

.city-select-tab>a:last-child.active {
  border-right: 1px solid #ccc;
}

.city-select-content {
  width: 100%;
  min-height: 10px;
  background-color: #fff;
  padding: 10px 15px;
  box-sizing: border-box;
}

.city-select {
  font-size: 13px;
}

.city-select dl {
  line-height: 2;
  clear: both;
  padding: 3px 0;
  margin: 0;
}

.city-select dt {
  position: absolute;
  width: 2.5em;
  font-weight: 500;
  text-align: right;
  line-height: 2;
}

.city-select dd {
  margin-left: 0;
  line-height: 2;
}

.city-select.province dd {
  margin-left: 3em;
}

.city-select a {
  display: inline-block;
  padding: 0 10px;
  outline: 0;
  text-decoration: none;
  white-space: nowrap;
  margin-right: 2px;
  text-decoration: none;
  color: #333;
  cursor: pointer;
}

.city-select a:hover,
.city-select a:focus {
  background-color: #f1f8ff;
  border-radius: 2px;
  color: #e98930;
}

.city-select a.active {
  background-color: #ffe6cf;
  color: #e98930;
  border-radius: 2px;
}