}

.nav_main .jsddmli a {
  display: block;
  height: 48px;
  background: url(../../images/static/line.jpg) no-repeat;
  color: #000;
  font-size: 16px;
}

.nav_main .jsddmli2 a {
  display: block;
  height: 48px;
  color: #000;
  font-size: 16px;
}

.nav .all-goods {
