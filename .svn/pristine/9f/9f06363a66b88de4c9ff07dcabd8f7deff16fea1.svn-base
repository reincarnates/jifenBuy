@charset "utf-8";

/* 头部 */
.head{
  width: 100%;
  height: 170px;
  background-color: #ffffff;
}

.head-top{
  width: 100%;
  height: 38px;
  background-color: #f5f5f5;
  border-bottom: 1px solid #e2e2e2;
}

.head-top-min{
  width: 1200px;
  height: 38px;
  margin: 0 auto;
  line-height: 38px;
}

.not-login{
  line-height: 38px;
}

.login-success-wrapper{
  float: left;
  display: flex;
  align-items: center;
}

.address{
  width: 58px;
  height: 36px;
  font-size: 12px;
  color: #666;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #f5f5f5;
  box-sizing: border-box;
  position: relative;
}

.address img{
  margin-right: 4px;
}

.cover {
  width: 58px;
  height: 2px;
  background-color: #fff;
  position: absolute;
  bottom: -1px;
  border-left: 1px solid #d0d0d0;
  border-right: 1px solid #d0d0d0;
  z-index: 100;
  display: none;
}

.home-city {
  position: absolute;
  top: 34px;
  left: -1px;
  width: 314px;
  height: 233px;
  background-color: #fff;
  border: 1px solid #d0d0d0;
  z-index: 99;
  box-sizing: border-box;
  display: none;
}

.city-select-content {
  width: 100%;
  min-height: 10px;
  background-color: #fff;
  padding: 10px 15px;
  box-sizing: border-box;
}

.city-selects {
  font-size: 12px;
  color: #c5c5c5;
}

.city-selects dl {
  line-height: 2;
  clear: both;
  padding: 3px 0;
  margin: 0;
  border-bottom: 1px dotted #ededed;
}

.city-selects a {
  display: inline-block;
  padding: 2px 13px;
  outline: 0;
  text-decoration: none;
  white-space: nowrap;
  margin-right: 2px;
  text-decoration: none;
  cursor: pointer;
}

.city-selects a:hover{
  background-color: #fe4600;
  color: #fff;
}

.login-name{
  font-size: 12px;
  color: #666;
  margin-left: 55px;
}

.sign-out{
  font-size: 12px;
  color: #666;
  margin-left: 30px;
}

.sign-out:hover{
  text-decoration: underline;
}

.head-top-min .key{
  float: right;
  /* padding: 10px 0; */
}

.head-top-min .key span{
  /* margin: 0 10px; */
  display: block;
  width: 105px;
  text-align: center;
  background: url(../../images/static/inhert_03.jpg);
  background-repeat:  no-repeat;
  background-position:  104px 7px;
  float: left;
  position: relative;
}

/* .head-top-min .key span:first-child{
  background: none;
} */

.login-success{
  display: none;
}

.down{
  position: absolute;
  top: 50%;
  right: 5px;
  width: 15px;
  margin-top: -3px;
  height: 7px;
  background: url(../../images/static/drop-arrow.png) no-repeat;
  z-index: 100;
}

.head-top-min .key span a{
  color: #666;
  position:relative;
  z-index: 100;
}

.head-top-min .key span a:hover{
  color: #e98930;
}

.myshapcard{
  background: url(../../images/static/car.png) no-repeat;
  padding-left: 35px;
}

.head-top-min .key span b{
  color: #fff;
  background-color: #ff7e00;
  padding: 0 5px;
  font-size: 13px;
  border-radius: 10px;
  font-weight: normal;
  top: -1px;
}

.head_mid{
  width: 1200px;
  height: 100px;
  padding-top: 32px;
  z-index: 10;
  margin: 0 auto;
}

.head_mid a img{
  float: left;
  margin-left: -15px;
  margin-right: 20px;
}

.search{
  float: left;
  margin-top: 13px;
  margin-left: 235px;
  position: relative;
}

.search-input-wrapper{
  /* float: left; */
  position: relative;
}

.search-input-left{
  width: 390px;
  height: 42px;
  line-height: 42px;
  border: 1px solid #ffb030;
  float: left;
  padding-left: 18px;
  color: #555555;
  outline: none;
}

.search-input-right{
  width: 55px;
  height: 42px;
  background: url(../../images/static/search.png) no-repeat 15px 11px;
  background-color: #ffb22c;
  cursor: pointer;
  float: left;
}

.hot{
  width: 400px;
  height: 30px;
  line-height: 30px;
  padding-left: 20px;
  overflow: hidden;
}

.hot span {
  font-style: italic;
  color: #666666;
}

.search .hot a {
  margin-right: 10px;
}

.search .hot a:hover {
  color: #e98930;
}

.shop_cart{
  width: 125px;
  height: 42px;
  border: 1px solid #ffb030;
  /* background-color: #fffcf6; */
  position: relative;
  text-align: center;
  line-height: 42px;
  color: #ffb030;
  font-size: 14px;
  box-sizing: border-box;
  float: left;
  margin-top: 13px;
  margin-left: 44px;
  background-image: url(../../images/static/cart.png);
  background-repeat: no-repeat;
  background-position: 20px 8px;
}

.shop_cart_word{
  margin-left: 38px;
  letter-spacing: 3px;
}

.total{
  float: left;
  font-size: 12px;
}

.total span{
  color: #333;
  font-size: 14px;
}

.search-select-small{
  width: 123px;
  height: 10px;
  background-color: #fff;
  z-index: 889;
  position: absolute;
  bottom: -2px;
  box-sizing: border-box;
  display: none;
}

.search-cart-select{
  width: 310px;
  height: 273px;
  background-color: #fff;
  position: absolute;
  right: -1px;
  top: 41px;
  z-index: 888;
  box-sizing: border-box;
  overflow-y: auto;
  border: 1px solid #fff;
  display: none;
}

.search-bg{
  background-color: #fff;
  border-color: #ffb22c;
}

.small-bg{
  border-color: #ffb22c;
  display: block;
}

.search-cart-select-head{
  width: 100%;
  height: 55px;
  line-height: 55px;
  font-size: 14px;
  color: #333;
  padding-left: 10px;
  box-sizing: border-box;
  text-align: left;
}

.search-cart-goods-all{
  width: 100%;
  padding-left: 5px;
  box-sizing: border-box;
}

.search-cart-goods-item{
  width: 100%;
  height: 63px;
  border-bottom: 1px dotted #d7d7d7;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.search-cart-goods-item:last-child{
  margin-bottom: 36px;
}

.search-cart-all-price{
  width: 308px;
  height: 36px;
  line-height: 36px;
  position: absolute;
  bottom: -273px;
  left: -185px;
  background-color: #e5e5e5;
  color: #454545;
  font-size: 14px;
  padding: 0 5px;
  box-sizing: border-box;
  z-index: 999;
  display: none;
}

.go-cart{
  width: 67px;
  height: 23px;
  background-color: #ffb22c;
  border: 1px solid #ffb22c;
  color: #fff;
  border-radius: 3px;
  text-align: center;
  line-height: 21px;
  float: right;
  margin-top: 7px;
}

.goods-img{
  width: 54px;
  height: 54px;
  border: 1px solid #eee;
  box-sizing: border-box;
}

.goods-account{
  width: 130px;
  font-size: 12px;
  line-height: 19px;
  margin-left: 3px;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}

.goods-account a {
  color: #333;
}

.goods-account a:hover {
  color: #ffb030;
}

.cart-delete:hover{
  color: #ffb030;
}

.cart-price-del{
  width: 95px;
  height: 57px;
  font-size: 12px;
  color: #343434;
}

.cart-price{
  font-weight: bold;
  line-height: 20px;
  text-align: right;
}

.cart-delete {
  text-align: right;
}

.nav{
  width: 100%;
  height: 48px;
  border-bottom: 2px solid #ffb22c;
  clear: both;
  font-size: 16px;
  color: #ffffff;
