@charset "utf-8";

.store-collection-wrapper{
  width: 980px;
  background-color: #fff;
  margin-left: 19px;
  box-sizing: border-box;
  float: left;
}

.account-head-wrapper {
  width: 100%;
  height: 30px;
  line-height: 30px;
  border-bottom: 1px dashed #e5e5e5;
  padding-left: 20px;
  box-sizing: border-box;
  font-size: 12px;
  color: #666;
}

.account-head-wrapper a:last-child {
  color: #333;
}

.store-collection-content{
  width: 100%;
  padding: 0 22px;
  box-sizing: border-box;
}

.store-collection-head{
  width: 100%;
  height: 42px;
  /* border: 1px solid #e2e2e2;
  background-color: #f5f5f5; */
  border-bottom: 1px solid #e2e2e2;
  margin-top: 15px;
  box-sizing: border-box;
}

.store-collection-head-item{
  width: 160px;
  height: 42px;
  text-align: center;
  line-height: 40px;
  color: #333;
  font-size: 16px;
  /* border-right: 1px solid #ddd; */
  cursor: pointer;
  float: left;
}

.store-collection-head-item-active{
  border-bottom: 2px solid #ffb030;
  color: #ffb030;
  background-color: #fff;
}

.store-collection-show{
  display: none;
}

.store-collection-hide{
  display: block;
}

.store-collection-item{
  width: 100%;
  margin-top: 30px;
  border-bottom: 1px dashed #ddd;
}

.store-collection-item:last-child{
  border-bottom: none;
}

.store-collection-item-left{
  width: 105px;
  height: 314px;
  float: left;
  position: relative;
}

.store-collection-item-left-img{
  width: 82px;
  height: 82px;
  border: 1px solid #ddd;
}

.store-collection-item-left-img img{
  width: 100%;
}

.store-collection-item-left-word{
  line-height: 32px;
  color: #333;
  font-size: 14px;
  margin-left: 19px;
  cursor: pointer;
}

.store-collection-item-right{
  width: 830px;
  height: 314px;
  float: left;
}

.store-collection-item-right-top{
  width: 100%;
  height: 41px;
}

.store-collection-item-right-name{
  float: left;
  font-size: 16px;
  font-weight: bold;
}

.store-collection-item-right-name a{
  color: #333;
}

.store-collection-item-right-name a:hover{
  text-decoration: underline;
}

.store-collection-item-right-more{
  float: right;
  color: #333;
  font-size: 14px;
  cursor: pointer;
}

.store-collection-item-right-more a:hover{
  text-decoration: underline;
}

.store-collection-item-right-more span{
  display: inline-block;
  width: 16px;
  height: 16px;
  background-color: #666;
  border-radius: 50%;
  text-align: center;
  line-height: 16px;
  color: #fff;   
  font-family: '宋体';
  font-size: 14px;
}

.store-collection-item-right-content{
  width: 100%;
}

.store-collection-item-right-content-item{
  width: 192px;
  height: 238px;
  border: 1px solid #ddd;
  margin-right: 20px;
  float: left;
}

.store-collection-item-right-content-item:last-child{
  margin-right: 0;
}

.store-collection-item-right-content-item-img{
  width: 100%;
  height: 171px;
  padding: 13px 24px;
  box-sizing: border-box;
}

.store-collection-item-right-content-item-img img{
  width: 100%;
}

.store-collection-item-right-content-item-word{
  /* height: 28px;
  line-height: 28px; */
  font-size: 12px;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  padding: 0 9px;
  box-sizing: border-box;
}

.store-collection-item-right-content-item-word a:hover{
  color: #e98930;
  text-decoration: underline;
}

.store-collection-item-right-content-item-price{
  padding: 0 12px;
  box-sizing: border-box;
  line-height: 37px;
}

.store-collection-new-price{
  color: #ff0000;
  font-size: 16px;
}

.store-collection-old-price{
  color: #666;
  font-size: 12px;
  text-decoration: line-through;
}

.cancel-collection{
  width: 81px;
  height: 23px;
  background-color: #e4e4e4;
  text-align: center;
  line-height: 23px;
  color: #333;
  font-size: 14px;
  /* border-radius: 25px; */
  /* margin-left: 5px; */
  cursor: pointer;
  position: absolute;
  top: 85px;
  display: none;
}

.seat{
  width: 100%;
  height: 50px;
  background-color: #e9e9e9;
}

.cancel-collection-mask-wrapper{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0, 0, 0, .3);
  z-index: 999;
  display: none;
}

.cancel-collection-mask{
  width: 527px;
  height: 236px;
  background-color: #fff;
  position: fixed;
  left: 50%;
  top: -3%;
  margin-left: -263px;
  margin-top: -210px;
}

.cancel-collection-mask-head{
  width: 100%;
  line-height: 53px;
  border-bottom: 1px dashed #d1d1d1;
  padding: 0 30px;
  box-sizing: border-box;
}

.cancel-collection-mask-head span{
  color: #999;
  font-size: 18px;
}

.cancel-collection-mask-head img{
  float: right;
  margin-top: 18px;
  cursor: pointer;
}

.cancel-collection-mask-content{
  color: #333;
  font-size: 16px;
  text-align: center;
  line-height: 28px;
}

.cancel-collection-mask-word{
  margin-top: 18px;
}

.cancel-collection-mask-btn{
  width: 255px;
  margin: 0 auto;
  margin-top: 25px;
}

.cancel-collection-mask-btn-left{
  width: 105px;
  height: 43px;
  background-color: #ffb030;
  text-align: center;
  line-height: 43px;
  color: #fff;
  font-size: 16px;
  float: left;
  margin-right: 45px;
  cursor: pointer;
}

.cancel-collection-mask-btn-left:hover{
  opacity: .8;
}

.cancel-collection-mask-btn-right{
  width: 105px;
  height: 43px;
  border: 1px solid #d1d1d1;
  text-align: center;
  line-height: 41px;
  color: #333;
  font-size: 14px;
  float: left;
  cursor: pointer;
}

.cancel-collection-mask-btn-right:hover{
  background-color: #ffb030;
  border-color: #ffb030;
  color: #fff;
}

.goods-collection-wrapper{
  width: 100%;
  margin-top: 26px;
  /* padding: 0 20px; */
  padding-left: 20px;
  box-sizing: border-box;
}

.goods-collection-item{
  width: 220px;
  height: 330px;
  border: 1px solid #e2e2e2;
  margin-right: 12px;
  margin-bottom: 21px;
  position: relative;
  float: left;
}

.goods-collection-item:hover{
  border-color: #ffb030;
  box-shadow: 0 5px 10px 1px rgba(0, 0, 0, .1);
}

.goods-collection-item:nth-child(4n+4) {
  margin-right: 0;
}

.goods-collection-item-img{
  width: 100%;
  height: 220px;
  position: relative;
}

.goods-collection-item-img2{
  width: 100%;
  height: 220px;
  position: relative;
  opacity: .5;
}

.goods-collection-item-opacity{
  opacity: .5;
}

.goods-collection-item-img img{
  width: 100%;
}

.goods-collection-item-img2 img{
  width: 100%;
}

.goods-collection-item-status{
  width: 100%;
  line-height: 31px;
  background-color: rgba(0, 0, 0, .3);
  position: absolute;
  bottom: 2px;
  text-align: center;
  color: #fff;
}

.goods-collection-item-word{
  width: 173px;
  line-height: 23px;
  font-size: 14px;
  margin-left: 8px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 7px;
}

.goods-collection-item-word a{
  color: #333;
  font-weight: bold;
}

.goods-collection-item-word a:hover{
  color: #e98930;
  text-decoration: underline;
}

.goods-collection-item-price{
  width: 100%;
  line-height: 23px;
  text-align: center;
}

.goods-collection-item-new-price{
  color: #ff2121;
  font-size: 14px;
}

.goods-collection-item-old-price{
  color: #999;
  font-size: 12px;
  text-decoration: line-through;
}

.goods-collection-item-btn{
  width: 197px;
  height: 28px;
  border: 1px solid #e2e2e2;
  background-color: #f5f5f5;
  line-height: 26px;
  color: #333;
  text-align: center;
  font-size: 12px;
  margin: 0 auto;
  margin-top: 10px;
}

.goods-collection-item-btn-left{
  width: 99px;
  border-right: 1px solid #e2e2e2;
  cursor: pointer;
  float: left;
}

.goods-collection-item-btn-right{
  width: 96px;
  cursor: pointer;
  float: left;
}

.goods-collection-item-btn-right2{
  width: 96px;
  cursor: pointer;
  float: left;
}

.goods-collection-item-delete-mask{
  width: 219px;
  height: 328px;
  background-color: rgba(0, 0, 0, .7);
  position: absolute;
  top: 0;
  display: none;
}

.goods-collection-item-delete-mask-word{
  text-align: center;
  line-height: 32px;
  color: #fff;
  font-size: 14px;
  margin-top: 103px;
}

.goods-collection-item-delete-mask-btn{
  width: 116px;
  height: 20px;
  font-size: 14px;
  text-align: center;
  margin: 0 auto;
  margin-top: 11px;
}

.goods-collection-item-delete-mask-btn-left{
  width: 48px;
  height: 20px;
  background-color: #ffb030;
  text-align: center;
  line-height: 20px;
  border-radius: 1px;
  margin-right: 20px;
  color: #fff;
  float: left;
  cursor: pointer;
}

.goods-collection-item-delete-mask-btn-right{
  width: 48px;
  height: 20px;
  background-color: #fff;
  text-align: center;
  line-height: 20px;
  border-radius: 1px;
  color: #333;
  float: left;
  cursor: pointer;
}

.goods-collection-item-notgoods-mask{
  width: 219px;
  height: 328px;
  background-color: rgba(0, 0, 0, .7);
  position: absolute;
  top: 0;
  line-height: 328px;
  text-align: center;
  color: #fff;
  font-size: 16px;
  display: none;
}