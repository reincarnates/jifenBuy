  outline: none;
  padding-left: 10px;
  background: url(../../images/static/select_cancel_order.png) no-repeat;
  background-position: 160px 11px;
  cursor: pointer;
}

.cacel-order-mask-selet-cause-content{
  width: 181px;
  border-style: solid;
  border-color: #d1d1d1;
