.cacel-order-mask-wrapper{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, .3);
  z-index: 999;
  display: none;
}

.cacel-order-mask{
  width: 527px;
  height: 316px;
  background-color: #fff;
  position: fixed;
  top: -50%;
  left: 50%;
  margin-left: -263px;
  margin-top: -158px;
}

.cacel-order-mask-head{
  width: 100%;
  height: 53px;
  border-bottom: 1px dashed #d1d1d1;
